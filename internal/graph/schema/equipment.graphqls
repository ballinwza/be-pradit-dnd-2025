type Equipment {
  id: String!
  characterId: String!
  armor: Armor!
  rightHanded: Weapon!
  leftHanded: Weapon!
}

type WatchEquipment {
  id: String
  characterId: String
  armor: String
  rightHanded: String
  leftHanded: String
}

extend type Subscription {
  watchEquipmentByCharacterId(characterId: String!): Equipment!
}

extend type Query {
  equipmentByCharacterId(characterId: String!): Equipment!
}
